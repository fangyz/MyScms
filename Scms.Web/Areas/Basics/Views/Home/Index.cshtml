@{
    ViewBag.Title = "";
    Layout = null;
}
<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <script type="text/javascript">
        function hideShowFrame() {
            if (document.getElementById("middenFram").cols == "193,*") {
                document.getElementById("middenFram").cols = "0,*";
            }
            else {
                document.getElementById("middenFram").cols = "193,*"
            }
        }
    </script>
</head>
<frameset rows="57,*,30" cols="*" frameborder="no" border="0" framespacing="0">
    <frame src="/Basics/Home/Top" name="topFrame" scrolling="No" noresize="noresize" id="topFrame" title="topFrame" />
    <frameset cols="193,*" frameborder="no" border="0" framespacing="0" id="middenFram">
        <frame src="/Basics/Home/Left" name="leftFrame" scrolling="No" noresize="noresize" id="leftFrame" title="leftFrame" />
        <frame src="/Basics/Home/Right" name="mainFrame" id="mainFrame" title="mainFrame" />
    </frameset>
    <frame src="/Basics/Home/Footer" name="topFrame" scrolling="No" noresize="noresize" id="bootomFrame" title="topFrame" />
</frameset>
<noframes>
    <body>
    </body>
</noframes>
</html>
